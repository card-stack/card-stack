{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "../../dist/out-tsc",
		"types": [
			"node",
			"@nx/react/typings/cssmodule.d.ts",
			"@nx/react/typings/image.d.ts"
		]
	},
	"exclude": [
		"jest.config.ts",
		"src/**/*.spec.ts",
		"src/**/*.spec.tsx"
	],
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx"
	]
}